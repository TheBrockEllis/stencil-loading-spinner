{
  "name": "@thebrockellis/loading-spinner",
  "private": false,
  "version": "1.0.0",
  "description": "A simple loading spinner for use in a stenciljs or web component application",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "module": "dist/esm/es5/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prerender",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev"
  },
  "dependencies": {
    "@stencil/core": "next",
    "@stencil/router": "0.0.9"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheBrockEllis/stencil-loading-spinner"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheBrockEllis/stencil-loading-spinner/issues"
  },
  "homepage": "https://github.com/TheBrockEllis/stencil-loading-spinner"
}
